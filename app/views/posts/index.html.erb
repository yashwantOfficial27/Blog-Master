
<h1>Posts</h1>

<div id="posts">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Title</th>
        <th scope="col">Description</th>
        <th scope="col">created at </th>
        <th scope="col">Show Post</th>
      </tr>
    </thead>
      <tbody>
        <% @posts.each do |post| %>
          <%= render post %>
        <% end %>
      </tbody>
  </table>
</div>

<% if user_signed_in? %>
  <%= link_to "New post", new_post_path, class:"btn btn-primary" %>
<% end %>